var id = '<%= @review.id %>'
var reviewer_nurse_id = '<%= @review.reviewer_nurse_id %>'
var schedule_id = '<%= @review.schedule_id %>'
var review= '<%= @review.review %>'
var score = '<%= @review.score %>'
var url = location.href;
<!--var data = {-->



console.log(score)
if(score < 0 ){
  if( confirm('感情分析結果は' + score + '点でした。マイナスな表現が含まれますが、投稿しますか？') ){
    location.href = "/";
  }
}else{
  if( confirm('感情分析結果は' + score + '点でした。投稿しますか？') ){
    $.ajax({
    type: 'POST',
